{
	"name": "slidev-theme-check24",
	"version": "0.0.0",
	"keywords": [
		"slidev-theme",
		"slidev"
	],
	"engines": {
		"node": ">=14.0.0",
		"slidev": ">=0.19.3"
	},
	"scripts": {
		"build": "slidev build example.md",
		"dev": "slidev example.md --open",
		"export": "slidev export example.md",
		"screenshot": "slidev export example.md --format png"
	},
	"dependencies": {
		"@slidev/types": "^0.40.3",
		"codemirror-theme-vars": "^0.1.1",
		"less": "^4.1.3",
		"prism-theme-vars": "^0.2.4",
		"theme-vitesse": "^0.6.2"
	},
	"devDependencies": {
		"@slidev/cli": "^0.40.3"
	},
	"//": "Learn more: https://sli.dev/themes/write-a-theme.html",
	"slidev": {
		"colorSchema": "light",
		"highlighter": "all",
		"defaults": {
			"fonts": {
				"sans": "Arial",
				"mono": "Fira Code"
			},
			"hightlighter": "prism",
			"favicon": "https://www.check24.de/favicon-32x32.png"
		}
	}
}
